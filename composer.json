{
  "autoload": {
    "psr-4": {
      "Acme\\": "src"
    }
  },
  "require-dev": {
    "squizlabs/php_codesniffer": "3.*"
  },
  "scripts": {
    "check": [
      "@cs-check"
    ],
    "cs-check": "php vendor/bin/phpcs  --colors -p ./src ./tests",
    "cs-fix": "php vendor/bin/phpcbf --colors ./src ./tests"
  }
}
