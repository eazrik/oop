{
  "autoload": {
    "psr-4": {
      "Acme\\": "src"
    }
  },
  "require-dev": {
    "squizlabs/php_codesniffer": "3.*",
    "phpstan/phpstan": "^0.12.11",
    "phpunit/phpunit": "^9.0"
  },
  "scripts": {
    "check": [
      "@cs-check"
    ],
    "cs-check": "php vendor/bin/phpcs  --colors -p --standard=custom_ruleset.xml ./src",
    "cs-fix": "php  vendor/bin/phpcbf --colors --standard=custom_ruleset.xml ./src"
  }
}
